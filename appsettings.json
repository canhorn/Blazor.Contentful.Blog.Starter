{
    "ContentfulOptions": {
        "DeliveryApiKey": "",
        "PreviewApiKey": "",
        "SpaceId": "",
        "UsePreviewApi": false
    },
    "Site": {
        "Owner": "A. Blogger",
        "Title": "My new Blazor + Contentful blog site",
        "domain": "blazor-contentful-blog-starter.azurewebsites.net",
        "email": "example@example.com",
        "feedDescription": "RSS Feed for example.com"
    },
    "pageMeta": {
        "openGraph": {
            "twitterUser": "contentful"
        },
        "home": {
            "url": "https://localhost:5001",
            "slug": "/"
        },
        "blogIndex": {
            "url": "https://localhost:5001/blog",
            "slug": "/blog"
        },
        "blogIndexPage": {
            "slug": "/blog/page/{page:int}"
        },
        "post": {
            "slug": "/blog/{slug}"
        },
        "buildRss": {
            "url": "https://localhost:5001/buildrss",
            "slug": "/buildrss"
        },
        "notFound": {
            "url": "https://localhost:5001",
            "slug": "/404"
        }
    },
    "pagination": {
        "pageSize": 2,
        "recentPostsSize": 3
    },
    "menuLinks": [
        {
            "displayName": "Home",
            "path": "/"
        },
        {
            "displayName": "Blog",
            "path": "/blog"
        }
    ],
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
